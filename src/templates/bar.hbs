<!DOCTYPE html>
<html>
    <head>
        <title>{{ title }}</title>
        <style>
            html, body {
                width: 100%;
                height: 100%;
                margin: 0;
            }
            canvas {
                margin: 0 auto;
            }
        </style>
    </head>
    <body>
        <div id="chart"></div>
        <script src="https://cdn.plot.ly/plotly-latest.min.js"></script>
        <script>
            var trace1 = {
                y: {{{ array2 }}},
                type: 'box',
                name: 'Revenue',
                boxpoints: 'suspectedoutliers',
            };

            var trace2 = {
                y: {{{ array2 }}},
                type: 'box',
                name: 'Employees',
                boxpoints: 'suspectedoutliers'
            };

            var data = [trace1, trace2];

            Plotly.newPlot('chart', data);
        </script>
    </body>
</html>